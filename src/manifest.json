{
  "name": "GifHub",
  "version": "1.0.0",
  "description": "Markdown-friendly giphys in a jiffy",
  "manifest_version": 2,
  "permissions": ["storage", "declarativeContent"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  }
}